{
  "name": "@inventory/task-queue",
  "version": "2.0.0-beta.7",
  "author": "Jarid Margolin <jaridmargolin@gmail.com>",
  "description": "Extensible async task queue.",
  "homepage": "https://github.com/jaridmargolin/task-queue",
  "repository": "github:jaridmargolin/task-queue",
  "license": "MIT",
  "main": "dist/common/task-queue.js",
  "module": "dist/es/task-queue.js",
  "browser": "dist/task-queue.js",
  "types": "dist/types/task-queue.d.ts",
  "scripts": {
    "check-types": "tsc",
    "build": "yarn build-lib && yarn build-docs && yarn build-types",
    "build-docs": "typedoc ./src/task-queue.ts",
    "build-lib": "rollup -c",
    "build-types": "tsc -p tsconfig.build.json ",
    "format": "prettier-eslint --write \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "yarn build",
    "publish-cov": "codecov",
    "start": "jest --watch",
    "test": "yarn lint && yarn check-types && jest",
    "view-cov": "open-cli ./coverage/lcov-report/index.html",
    "view-docs": "open-cli ./docs/index.html"
  },
  "dependencies": {
    "core-js": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-typescript": "7.7.7",
    "@rollup/plugin-commonjs": "11.0.1",
    "@rollup/plugin-node-resolve": "7.0.0",
    "@types/jest": "24.0.25",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "codecov": "4.0.0-0",
    "delay": "4.3.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "4.0.6",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "opn-cli": "5.0.0",
    "p-defer": "3.0.0",
    "p-wait-for": "3.1.0",
    "prettier-eslint-cli": "4.7.1",
    "rollup": "1.29.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-terser": "5.1.3",
    "typedoc": "0.27.7",
    "typescript": "3.7.4"
  }
}
